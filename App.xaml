<Application x:Class="SignalProcessor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="ViewLogic\MainWindow.xaml"
             xmlns:l="clr-namespace:SignalProcessor.ViewLogic">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="StylesWindow" Source="Styles\Styles.xaml" />
                <ResourceDictionary x:Name="StylesHider" Source="Styles\StylesHider.xaml" />
                <ResourceDictionary x:Name="StylePopupPanel" Source="Styles\PanelStyle.xaml" />
                <ResourceDictionary x:Name="StyleWindow" Source="Styles\StyleWindow.xaml" />
                <ResourceDictionary x:Name="StyleLabelHeader" Source="Styles\LabelHeaderStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
        
        
    </Application.Resources>
</Application>
